{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-css": "sass --watch src/.:docs/",
    "watch-js": "watchify -t brfs src/index.js -o docs/index.js",
    "front-dev": "concurrently \"npm:watch-css\"  \"npm:watch-js\" ",
    "watch": "concurrently \"npm:watch-*\"",
    "build-js": "browserify src/index.js -o docs/index.js -p tinyify -t brfs",
    "build-css": "sass src/.:docs/ --style compressed",
    "build": "concurrently \"npm:build-*\""
  },
  "dependencies": {
    "@forgjs/noframework": "^2.0.0",
    "feather-icons": "^4.28.0",
    "highlight.js": "^10.0.2",
    "markdown-it": "^12.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-polyfill": "^6.26.0",
    "babelify": "^10.0.0",
    "brfs": "^2.0.2",
    "browserify": "^16.5.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^22.17.0",
    "sass": "^1.24.3",
    "tinyify": "^2.5.2",
    "watchify": "^3.11.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
